<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.pocusapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_pocus_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_pocus_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.PocusApp"
        tools:targetApi="31">
        <activity
            android:name=".TimersActivity"
            android:exported="false" />
        <activity
            android:name=".AboutActivity"
            android:exported="false" />
        <activity
            android:name=".WhatIsPomodoroActivity"
            android:exported="false" />
        <activity
            android:name=".ImportanceOfFocusActivity"
            android:exported="false" />
        <activity
            android:name=".WhatIsPocusActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>